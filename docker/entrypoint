#!/bin/sh

REST='\e[0m'
RED="\e[31m"

echo ""
echo "${RED}\n"
echo "  ######   ######    #####   ######               ###            ###  ##   ####    ##### ";
echo "   ##  ##  # ## #   ##   ##   ##  ##             ####             ##  ##    ##      ## ## ";
echo "   ##  ##    ##     #         ##  ##            ## ##             ## ##     ##      ##  ## ";
echo "   #####     ##      #####    #####            ##  ##             ####      ##      ##  ## ";
echo "   ## ##     ##          ##   ##               #######            ## ##     ##      ##  ## ";
echo "   ##  ##    ##     ##   ##   ##                   ##             ##  ##    ##      ## ## ";
echo "  #### ##   ####     #####   ####                  ##            ###  ##   ####    ##### ";
echo "${REST}\n"

# Execute Argument
exec "$@"
